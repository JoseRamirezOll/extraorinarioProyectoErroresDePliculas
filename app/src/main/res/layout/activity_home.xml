<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmento"
        android:layout_width="411dp"
        android:layout_height="659dp"
        android:name='com.example.extraorinarioproyectoerroresdepliculas.homeFragment'
        app:layout_constraintBottom_toTopOf="@+id/botonesnavegacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/botonesnavegacion"
        android:layout_width="408dp"
        android:layout_height="69dp"
        android:layout_marginStart="3dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/fragmento"
        app:layout_constraintVertical_bias="0.995"
        app:menu="@menu/menuinferior" />
</androidx.constraintlayout.widget.ConstraintLayout>